VPATH = src:ut_src
CFLAGS+= -Isrc -Iut_src
CFLAGS+= -Iut_stub

BUILD_DIR=build_ut/


CFLAGS+= -g -O0 -Werror -Wall -DUNITTEST


BINS+= ut_dcc_decoder
BINS+= ut_cv

BUILD_BINS=$(addprefix $(BUILD_DIR), $(BINS))

all: $(BUILD_BINS)
	echo $(BUILD_BINS)
	$(subst $(eval ) , && ,$(BUILD_BINS))


clean:
	rm -f build_ut/*



include build.mk

